        <ui:VisualElement name="titleContainer" class="upload-title-container">
            <ui:Label text="Latest WebGL Builds" name="lblTitle" class="upload-title-label" />
            <uie:ToolbarMenu name="menuHelp" class="upload-settings-dropdown" />
        </ui:VisualElement>
        <ui:ScrollView>
            <ui:VisualElement name="buildsList" class="build-list">
