.header-container {
    flex-direction: row;
    border-bottom-width: 1px;
    border-bottom-color: rgba(0, 0, 0, 0.2);
    background-color: rgba(255, 255, 255, 0.2);
}

.header-thumbnail {
    width: 64px;
    height: 64px;
    -unity-background-scale-mode: scale-to-fit;
    margin-left: 14px;
    margin-right: 14px;
    margin-top: 14px;
    margin-bottom: 14px;
}

.header-project-info {
    margin: 10px;
}

.header-project-name {
    -unity-font-style: bold;
    margin-top: 10px;
}

.header-user-email {
    margin-top: 2px;
}

.main-container {
    align-items: center;
    flex-grow: 1;
    justify-content: center;
}

.status-icon {
    width: 100px;
    height: 100px;
    margin-bottom: 8px;
    -unity-background-scale-mode: scale-to-fit;
}

.progress-bar {
    width: 200px;
}

.progress-label {
    -unity-font-style: bold;
    margin-top: 15px;
    margin-left: 30px;
    margin-right: 30px;
    font-size: 14px;
    -unity-text-align: upper-center;
    white-space: normal;
}

.bottom-button {
    height: 26px;
    margin-left: 10px;
    margin-right: 10px;
    margin-top: 10px;
    margin-bottom: 10px;
}

.bottom-button-locate {
    margin-top: 0px;
}

.connect-link {
    color: rgb(61, 119, 194);
    -unity-font-style: bold;
    margin-left: 14px;
    margin-right: 14px;
    white-space: normal;
    -unity-text-align: upper-center;
}

.info-text {
    margin-top: 5px;
    white-space: normal;
    -unity-text-align: upper-center;
    margin-left: 30px;
    margin-right: 30px;
    margin-bottom: 5px;
    font-size: 12px;
    max-width: 300px;
}

.main-container-light {
    background-color: rgba(255, 255, 255, 0.2);
    border-bottom-width: 1px;
    border-bottom-color: rgba(0, 0, 0, 0.2);
}

.bottom-button-blue {
    -unity-font-style: bold;
}

.build-container {
    margin-bottom: 0;
    background-color: rgba(255, 255, 255, 0.2);
    border-left-color: rgba(0, 0, 0, 0.2);
    border-right-color: rgba(0, 0, 0, 0.2);
    border-top-color: rgba(0, 0, 0, 0.2);
    border-bottom-color: rgba(0, 0, 0, 0.2);
    border-left-width: 0px;
    border-right-width: 0px;
    border-top-width: 0px;
    border-bottom-width: 1px;
}

.build-header {
    flex-direction: row;
    margin-bottom: 10px;
    margin-left: 10px;
    margin-right: 10px;
    margin-top: 10px;
}

.build-text-labels {
    flex-grow: 1;
}

.build-name {
    font-size: 13px;
    -unity-font-style: bold;
    flex-wrap: wrap;
    -unity-text-align: upper-left;
    white-space: normal;
}

.build-date {
    margin-top: 3px;
    opacity: 0.7;
    white-space: normal;
}

.build-folder-button {
    width: 16px;
    -unity-font-style: bold;
    height: 16px;
    margin-left: 10px;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    background-color: rgba(0, 0, 0, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
}

.build-folder-button:hover {
    background-color: rgba(255, 255, 255, 0.2);
}

.build-delete-button {
    -unity-font-style: bold;
    width: 16px;
    height: 16px;
    -unity-background-scale-mode: scale-and-crop;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    margin-left: 0;
    background-color: rgba(0, 0, 0, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
}

.build-delete-button:hover {
    background-color: rgba(255, 255, 255, 0.2);
}

.build-share-button {
    height: 24px;
    margin-left: 10px;
    margin-bottom: 10px;
    margin-top: 0;
    margin-right: 10px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
}

.upload-main-container {
    flex-grow: 1;
}

.upload-title-label {
    flex-grow: 1;
    margin-left: 10px;
    margin-right: 15px;
    margin-top: 0;
    margin-bottom: 0;
    justify-content: flex-start;
    -unity-text-align: middle-left;
    font-size: 12px;
}

.upload-settings-dropdown {
    border-right-width: 0px;
    flex-direction: column;
    width: 24px;
    height: 24px;
}

.upload-title-container {
    flex-direction: row;
    background-color: rgba(255, 255, 255, 0.16);
    border-bottom-width: 1px;
    border-bottom-color: rgba(0, 0, 0, 0.2);
    height: 25px;
    max-height: 25px;
    min-height: 25px;
}

.upload-footer-container {
    border-top-color: rgba(0, 0, 0, 0.2);
    border-top-width: 1px;
    min-height: 46px;
}

.build-list {
    flex-direction: column;
    flex-grow: 1;
    flex-shrink: 1;
    padding-bottom: 15px;
}

.header-thumbnail {
    background-image: resource('Light/icon-image');
}

.upload-settings-dropdown {
    background-image: resource('Light/icon-settings');
}

.build-folder-button {
    background-image: resource('Light/icon-folder');
}

.build-delete-button {
    background-image: resource('Light/icon-delete');
}

.status-icon-error {
    background-image: resource('Light/icon-error');
}

.status-icon-webgl-module {
    background-image: resource('Light/icon-webgl-module');
}

.status-icon-build-and-share {
    background-image: resource('Light/icon-build-and-share');
}

.status-icon-uploading {
    background-image: resource('Light/icon-uploading');
}

.status-icon-account {
    background-image: resource('Light/icon-account');
}

.status-icon-success {
    background-image: resource('Light/icon-success');
}

.status-icon-processing {
    background-image: resource('Light/icon-processing');
}
